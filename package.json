{
  "name": "aetherium",
  "version": "1.1.0",
  "private": true,
  "description": "A web-based twisty puzzle timer with real-time syncing of times across devices.",
  "author": "Matthew Stiles",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:stylelint:fix",
    "lint:eslint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore ./src",
    "lint:eslint:fix": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore --fix ./src",
    "lint:stylelint": "stylelint --ignore-path .gitignore \"src/**/*.(css|vue)\"",
    "lint:stylelint:fix": "stylelint --ignore-path .gitignore --fix \"src/**/*.(css|vue)\"",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "firebase": "^9.6.7",
    "highcharts": "^10.0.0",
    "javascript-state-machine": "^3.1.0",
    "jquery": "^3.5.1",
    "jsonschema": "^1.2.6",
    "moment": "^2.26.0",
    "pinia": "^2.0.11",
    "stackmat": "^1.0.0",
    "url": "^0.11.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^4.0.4",
    "@stilesdev/eslint-config-vue": "^0.5.5",
    "@stilesdev/stylelint-config-vue": "^0.5.5",
    "@types/bootstrap": "^3.4.0",
    "@types/jquery": "^3.3.38",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.0",
    "eslint": "^8.44.0",
    "stylelint": "^14.16.1",
    "typescript": "^4.5.0",
    "vite": "^2.8.4",
    "vitest": "^0.7.4",
    "vue-tsc": "^0.33.2-patch.1"
  },
  "overrides": {
    "stylelint-config-recommended-vue": {
      "stylelint-config-recommended": "^9.0.0"
    }
  }
}
