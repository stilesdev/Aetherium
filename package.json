{
  "name": "aetherium",
  "version": "1.1.0",
  "private": true,
  "description": "A web-based twisty puzzle timer with real-time syncing of times across devices.",
  "author": "Matthew Stiles",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:stylelint:fix",
    "lint:eslint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore ./src",
    "lint:eslint:fix": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore --fix ./src",
    "lint:stylelint": "stylelint --ignore-path .gitignore \"src/**/*.(css|vue)\"",
    "lint:stylelint:fix": "stylelint --ignore-path .gitignore --fix \"src/**/*.(css|vue)\"",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "firebase": "^9.23.0",
    "highcharts": "^10.3.3",
    "javascript-state-machine": "^3.1.0",
    "jquery": "^3.7.0",
    "jsonschema": "^1.4.1",
    "moment": "^2.29.4",
    "pinia": "^2.1.4",
    "stackmat": "^1.1.1",
    "url": "^0.11.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@stilesdev/eslint-config-vue": "^0.5.5",
    "@stilesdev/stylelint-config-vue": "^0.5.5",
    "@types/bootstrap": "^3.4.0",
    "@types/jquery": "^3.5.16",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.33.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "stylelint": "^14.16.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.4"
  },
  "overrides": {
    "stylelint-config-recommended-vue": {
      "stylelint-config-recommended": "^9.0.0"
    }
  }
}
