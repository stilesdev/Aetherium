{
  "name": "aetherium",
  "version": "1.1.0",
  "private": true,
  "description": "A web-based twisty puzzle timer with real-time syncing of times across devices.",
  "author": "Matthew Stiles",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "firebase": "^9.6.7",
    "highcharts": "^9.3.3",
    "javascript-state-machine": "^3.1.0",
    "jquery": "^3.5.1",
    "jsonschema": "^1.2.6",
    "moment": "^2.26.0",
    "stackmat": "^1.0.0",
    "url": "^0.11.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^4.0.4",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/bootstrap": "^3.4.0",
    "@types/jquery": "^3.3.38",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.0",
    "eslint": "^7.5.0",
    "eslint-plugin-vue": "^8.4.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.0",
    "vite": "^2.8.4",
    "vitest": "^0.6.0",
    "vue-tsc": "^0.32.1"
  }
}
